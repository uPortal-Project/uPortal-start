{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["esnext", "DOM"],
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": false,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true
  },
  "include": ["tests"]
}
